{
  "name": "static.rocketbase.io",
  "version": "2.0.0",
  "description": "Static generator for company website",
  "scripts": {
    "start": "eleventy --serve & npx tailwindcss -i src/styles/tailwind.css -c tailwind.config.js -o _site/style.css --watch",
    "build": "ELEVENTY_PRODUCTION=true eleventy && NODE_ENV=production npx tailwindcss -i src/styles/tailwind.css -c styles/tailwind.config.js -o _site/style.css --minify"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rocketbase-io/static.rocketbase.io.git"
  },
  "author": {
    "name": "Marten Prie√ü",
    "email": "marten@rocketbase.io",
    "url": "https://www.rocketbase.io"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^1.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "html-minifier": "^4.0.0",
    "tailwindcss": "^3.0.12"
  }
}
